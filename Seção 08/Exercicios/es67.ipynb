   "source": [
    "def rotina(vetor: list, tamanho: int):\n",
    "    getchar = None\n",
    "    while True:\n",
    "        if len(vetor) == tamanho:\n",
    "            break\n",
    "        getchar = input()\n",
    "        if getchar == '':\n",
    "            break\n",
    "        vetor.append(getchar)\n",
    "    return vetor"
   ],

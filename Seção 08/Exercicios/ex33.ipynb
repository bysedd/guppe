    "def soma_algarismo_fatorial(n: int):\n",
    "    from math import factorial\n",
    "    x = str(factorial(n))\n",
    "    soma = 0\n",
    "    for a in x:\n",
    "        soma += int(a)\n",
    "    return soma"
   ],

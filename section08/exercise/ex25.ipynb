   "source": [
    "def serie(n: int = 5):\n",
    "    s = 0\n",
    "    p = ''\n",
    "    print('S = ', end='')\n",
    "    for i in range(1, n + 1):\n",
    "        n1 = i ** 2 + 1\n",
    "        n2 = i + 3\n",
    "        s += n1 / n2\n",
    "        p += f'{n1}/{n2} + '\n",
    "    print(f'{p[:len(p) - 3]} = {s:.2f}')"
   ],

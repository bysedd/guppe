   "source": [
    "def pares_impares(*vetor: int):\n",
    "    a = []\n",
    "    b = []\n",
    "    for n in vetor:\n",
    "        if n % 2 == 0:\n",
    "            a.append(n)\n",
    "        else:\n",
    "            b.append(n)\n",
    "    return a, b"
   ],

    "def matriz_4x4(matriz: list):\n",
    "    import numpy as np\n",
    "    matriz = np.array(matriz)\n",
    "    s = 0\n",
    "    for l in matriz:\n",
    "        for n in l:\n",
    "            if n > 10:\n",
    "                s += 1\n",
    "    return s"
   ]
